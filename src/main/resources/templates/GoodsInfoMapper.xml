<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.boot.springbootdemo.dao.GoodsInfoMapper">

    <select id="selectById" resultType="com.boot.springbootdemo.entity.goods.GoodsInfoBean">
        select * from tbl_goods_info
        where id = #{id}
    </select>

</mapper>